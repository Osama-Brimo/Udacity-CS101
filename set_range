def bigger(a,b):
    if a>b:
        return a
    else:
        return b

def biggest(a,b,c):
    return bigger(c,bigger(a,b))
    
def smaller(a,b):
    if a<b:
        return a
    else:
        return b

def smallest(a,b,c):
    return smaller(c,smaller(a,b))

def set_range(a,b,c):
    return biggest(a,b,c)-smallest(a,b,c)
