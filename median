def bigger(a,b):
    if a > b:
        return a
    else:
        return b

def biggest(a,b,c):
    return bigger(a,bigger(b,c))

def median(a,b,c):
    if biggest (a,b,c)==a:
        return bigger(b,c)
    if biggest (a,b,c)==b:
        return bigger(a,c)
    if biggest (a,b,c)==c:
        return bigger(a,b)
