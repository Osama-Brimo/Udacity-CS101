def is_leap_baby(day,month,year):
    leapdays=0
    if year % 4!=0:
        leapdays=leapdays+0
    else:
        if year % 100!=0:
            leapdays=leapdays+1
        else:
            if year % 400!=0:
                leapdays=leapdays-1
            else:
                leapdays=leapdays+1
    if leapdays==1 and month==2 and day==29:
        return True
    else:
        return False
